{
  "name": "electron-browser",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "main": "electron/electron.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron-dev": "concurrently \"set BROWSER=none&&npm run dev\" \"wait-on http://localhost:5173 && npm run start-dev .\"",
    "start-dev": "electron . --inspect=5858"
  },
  "dependencies": {
    "concurrently": "^8.0.1",
    "electron": "^24.1.2",
    "electron-builder": "^23.6.0",
    "electron-is-dev": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "sass": "^1.62.0",
    "styled-components": "^5.3.9",
    "wait-on": "^7.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "vite": "^4.2.0"
  }
}
